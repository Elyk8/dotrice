#!/bin/sh
case $BLOCK_STATUS in
	# Top bar
	0) sb-clock-click ;;
	1) sb-battery-click ;;
	2) sb-volume-click ;;
	3) sb-microphone-click ;;
	4) sb-brightness-click ;;
	5) sb-redshiftstatus-click ;;
	7) rofi-bluetooth & ;;
	8) sb-pacpackages-click & ;;

	# Bottom bar
	11) sb-music-click ;;

	*) notify-send -t 2000 "Unknown status $BLOCK_STATUS clicked $BLOCK_BUTTON" & ;;
esac
